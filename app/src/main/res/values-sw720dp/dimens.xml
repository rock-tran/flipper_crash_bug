<!-- Example customization of dimensions originally defined in res/values/dimens.xml
         (such as screen margins) for screens with more than 600dp of available width. This
         would include 10" devices in landscape -->
<resources>
    <dimen name="small_text_size">14sp</dimen>
    <dimen name="normal_text_size">18sp</dimen>
    <dimen name="big_text_size">24sp</dimen>
</resources>