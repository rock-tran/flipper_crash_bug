<?xml version="1.0" encoding="utf-8"?>
<!-- Example customization of dimensions originally defined in res/values/dimens.xml
         (such as screen margins) for screens with more than 600dp of available width. This
         would include 7" devices in landscape -->
<resources>
    <dimen name="small_text_size">20sp</dimen>
    <dimen name="normal_text_size">30sp</dimen>
    <dimen name="big_text_size">36sp</dimen>
</resources>